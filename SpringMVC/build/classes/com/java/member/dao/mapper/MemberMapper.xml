<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.java.member.dao.mapper.MemberMapper">
	<insert id="member_insert" parameterType="com.java.member.dto.MemberDto">
		insert into member
		values(member_num_seq.nextval, #{id}, #{pw}, #{name}, #{jumin1}, #{jumin2}, #{email}, 'blank', #{addr}, #{job}, #{mailing}, #{interest}, 1, SYSDATE)
	</insert>
</mapper> 